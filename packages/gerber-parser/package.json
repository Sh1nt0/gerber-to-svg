{
  "name": "gerber-parser",
  "version": "0.0.1",
  "description": "A PCB Gerber/drill-file parser implemented as a Node transform-stream",
  "main": "lib/gerber-parser.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint lib/*.js test/*.js",
    "pretest": "npm run lint",
    "test": "istanbul cover --include-all-sources _mocha",
    "test-watch": "mocha --reporter dot --watch",
    "browser-test": "echo 'task not implemented' && exit 1",
    "browser-test-phantom": "echo 'task not implemented' && exit 1",
    "browser-test-sauce": "echo 'task not implemented' && exit 1",
    "ci": "npm run test && coveralls < ./coverage/lcov.info"
  },
  "precommit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcous/gerber-parser.git"
  },
  "keywords": [
    "PCB",
    "Gerber",
    "drill",
    "circuit",
    "board",
    "transform",
    "stream",
    "hardware",
    "electronics"
  ],
  "author": "Mike Cousins <mike@cousins.io> (http://cousins.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcous/gerber-parser/issues"
  },
  "homepage": "https://github.com/mcous/gerber-parser#readme",
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.2.1",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
    "pre-commit": "^1.1.1"
  }
}
